<h1 class="text-center">Update Place Event Relation</h1>
<%= simple_form_for(@place_event_relation) do |f| %>
  <%= f.error_notification %>
  <%= f.association :place %>
  <%= f.association :event %>
  <%= f.input :active, placeholder:"Active", label: "Active" %>
  <%= f.button :submit, "Update Place Event Relation", class: "btn btn-primary" %>
  <%= link_to "Cancel", :back, class: "btn btn-warning" %>
  <%= link_to "Place Event Relations", place_event_relations_path, class: "btn btn-primary"  %>
<% end %>
