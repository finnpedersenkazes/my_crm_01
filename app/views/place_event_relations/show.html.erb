<h1 class="text-center">Place Event Relation</h1>
<p>
  <strong>Place: </strong>
  <%= link_to @place_event_relation.place.name, @place_event_relation.place %>
</p>
<p>
  <strong>Event: </strong>
  <%= link_to @place_event_relation.event.name, @place_event_relation.event %>
</p>
<p>
  <strong>Active: </strong>
  <%= @place_event_relation.active %>
</p>
<p class="text-center">
  <%= link_to "Edit", edit_place_event_relation_path(@place_event_relation), class: "btn btn-warning " %>
  <%= link_to "Delete", place_event_relation_path(@place_event_relation), method: :delete, class: "btn btn-danger" %>
  <%= link_to "Place Event Relations", place_event_relations_path, class: "btn btn-primary"  %>
</p>
