<h1 class="text-center">New Thing Event Relation</h1>
<%= simple_form_for(@thing_event_relation) do |f| %>
  <%= f.error_notification %>
  <%= f.association :thing %>
  <%= f.association :event %>
  <%= f.input :active, placeholder:"Active", label: false %>
  <%= f.button :submit, "Add Thing Event Relation", class: "btn btn-primary" %>
  <%= link_to "Thing Event Relations", thing_event_relations_path, class: "btn btn-primary"  %>
<% end %>
